{
  "name": "robotpy-extension",
  "displayName": "RobotPy Extension",
  "description": "A VS Code extension for managing RobotPy subcommands with selectable flags.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:robotpyExtension.setCommand",
    "onCommand:robotpyExtension.selectFlags",
    "onCommand:robotpyExtension.setWorkingDir",
    "onCommand:robotpyExtension.clearFlags",
    "onCommand:robotpyExtension.runCommand"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "robotpyExtension.setCommand",
        "title": "Set RobotPy Command"
      },
      {
        "command": "robotpyExtension.selectFlags",
        "title": "Select RobotPy Flags"
      },
      {
        "command": "robotpyExtension.setWorkingDir",
        "title": "Set Working Directory"
      },
      {
        "command": "robotpyExtension.clearFlags",
        "title": "Clear RobotPy Flags"
      },
      {
        "command": "robotpyExtension.runCommand",
        "title": "Run RobotPy Command"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "robotpyViewContainer",
          "title": "RobotPy",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "robotpyViewContainer": [
        {
          "id": "robotpyView",
          "name": "RobotPy Commands"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "vsce": "^2.10.0"
  }
}
